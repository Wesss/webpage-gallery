{
  "presets": ["@babel/env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "src": "./src",
          "app": "./src/app",
          "appChroma": "./src/appChroma",
          "compose": "./src/compose",
          "unit": "./src/unit",
          "icon": "./src/icon"
        }
      }
    ]
  ]
}
